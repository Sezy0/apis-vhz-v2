# Sample environment configuration for vinzhub-rest-api-v2

# Server
SERVER_HOST=0.0.0.0
SERVER_PORT=8080
SERVER_READ_TIMEOUT=15s
SERVER_WRITE_TIMEOUT=15s
SERVER_SHUTDOWN_TIMEOUT=30s

# App
APP_NAME=vinzhub-api
APP_ENV=development
APP_DEBUG=false
APP_VERSION=2.0.0

# Cache
CACHE_TYPE=redis
CACHE_TTL=5m

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0

# MySQL (for key_accounts)
DB_HOST=localhost
DB_PORT=3306
DB_NAME=vinzhub
DB_USER=root
DB_PASS=

# API Keys (comma-separated for multiple)
API_KEYS=your-api-key-here
